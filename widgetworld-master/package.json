  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build & concurrently --kill-others \"npm run serve-api\" \"npm run serve\"",
    "serve": "ng serve --disable-host-check --proxy-config proxy.conf.json",
    "build": "ng build --output-path=public",
    "serve-api": "node ./travis/bin/www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node build-scripts/fs-patch.js",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "tscov": "tscov -m NUMBER"
  },
  "husky": {
    "hooks": {
      "pre-push": "ng test --watch=false"
    }
  },
  "private": true,
